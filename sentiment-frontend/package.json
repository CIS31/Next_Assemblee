{
  "name": "sentiment-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && npm run copy-assets",
    "copy-assets": "npm run copy-public && npm run copy-static",
    "copy-public": "cp -r public .next/standalone/public || xcopy /E /I public .next\\standalone\\public",
    "copy-static": "cp -r .next/static .next/standalone/.next/static || xcopy /E /I .next\\static .next\\standalone\\.next\\static",
    "start": "node .next/standalone/server.js"

  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^6.4.8",
    "axios": "^1.8.4",
    "next": "^15.2.4",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-query": "^3.39.3",
    "recharts": "^2.15.1"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "typescript": "^5"
  }
}
